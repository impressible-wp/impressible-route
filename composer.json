{
    "name": "impressible/impressible-route",
    "description": "A routing framework for coding modern PHP in Wordpress Plugin.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/impressible-wp/impressible-route",
    "autoload": {
        "psr-4": {
            "Impressible\\ImpressibleRoute\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Koala Yeung",
            "email": "koalay@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wordpress/wordpress",
                "version": "6.8.3",
                "type": "wordpress-core",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/WordPress/WordPress/archive/refs/tags/6.8.3.zip"
                }
            }
        }
    ],
    "scripts": {
        "test:unit": "phpunit tests --colors=always  --coverage-text=coverage.txt --coverage-cobertura=coverage.xml",
        "test": [
            "composer run test:unit"
        ]
    },
    "require": {
        "php": ">=8.1",
        "guzzlehttp/psr7": "^2.2",
        "psr/container": "^2.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0",
        "codeception/module-phpbrowser": "^2.0",
        "phpunit/php-code-coverage": "^9.2",
        "wp-cli/wp-cli-bundle": "*",
        "wordpress/wordpress": "^6.8"
    },
    "autoload-dev": {
        "files": [
            "composer-dev-autoload.php",
            "vendor/wordpress/wordpress/wp-includes/class-wp-query.php",
            "vendor/wordpress/wordpress/wp-includes/class-wp-rewrite.php",
            "vendor/wordpress/wordpress/wp-includes/functions.php"
        ]
    }
}
